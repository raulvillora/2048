{
  "extends": [
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
      "react"
  ],
  "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "jest": true
  },
  "globals": {
      "expect": true,
      "jest": true
  },
  "rules": {
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 0
  }
}
